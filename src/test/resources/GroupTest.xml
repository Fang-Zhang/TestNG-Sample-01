<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="GroupTestSuite">
	<parameter name="url"
		value="https://opensource-demo.orangehrmlive.com/" />
	<test name="GroupTest">
		<groups>
			<define name="dailyBuild">
				<include name="sanity" />
				<include name="functional" />
			</define>
			<define name="weeklyBuild">
				<include name="smoke" />
				<include name="sanity" />
				<include name="functional" />
				<include name="regression" />
			</define>
			<run>
				<include name="weeklyBuild" />
				<!-- <include name="all" /> -->
			</run>
		</groups>
		<classes>
			<class name="com.testing.groupTest.GroupTest01" />
			<class name="com.testing.groupTest.GroupTest02" />
		</classes>
	</test>
</suite>
